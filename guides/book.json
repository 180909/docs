{
  "author": "Aeneas Rekkas",
  "gitbook": ">=3.2.3",
  "plugins": [
    "edit-link",
    "auto-summary",
    "etoc",
    "ga",
    "sitemap-basepath",
    "versions",
    "youtube"
  ],
  "pluginsConfig": {
    "auto-summary": {
      "parts": {
        "README.md": "Overview",
        "hydra/**/*.md": "ORY Hydra",
        "oathkeeper/**/*.md": "ORY Oathkeeper",
        "telemetry/*.md": "Telemetry"
      }
    },
    "edit-link": {
      "base": "https://github.com/ory/docs/tree/master/guides",
      "label": "Edit this page"
    },
    "etoc": {
      "h2lb": 2,
      "mindepth": 2,
      "maxdepth": 5,
      "notoc": false
    },
    "ga": {
      "token": "UA-71865250-1"
    },
    "sitemap-basepath": {
      "hostname": "https://www.ory.sh/",
      "basePath": "/docs/guides/"
    },
    "versions": {
      "gitbookConfigURL": "https://raw.githubusercontent.com/ory/docs/master/guides/book.json",
      "options": [
        {
          "value": "https://www.ory.sh/docs/guides/latest",
          "text": "latest"
        },
        {
          "value": "https://www.ory.sh/docs/guides/v0.11.12",
          "text": "0.11.12"
        }
      ]
    }
  }
}
